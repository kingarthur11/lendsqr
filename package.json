{
  "name": "lendspr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run build-ts",
    "build:prod": "npm run write:file && npm run build-ts",
    "serve": "node ./dist/src/Program.js",
    "start:prod": "npm run write:file && npm run build && npm run start",
    "start:dev": "npm run build && npm run watch",
    "migrate": "knex migrate:latest",
    "make": "knex migrate:make",
    "seed:make": "knex seed:make",
    "migrate:pro": "NODE_ENV=production knex db:migrate",
    "unmigrate:pro": "NODE_ENV=production knex db:migrate:undo:all",
    "unmigrate": "knex db:migrate:undo:all",
    "unmigrate-last": "knex db:migrate:undo",
    "seed": "npm run seed-undo && knex db:seed:all",
    "seed-undo": "knex db:seed:undo:all",
    "watch-node": "nodemon dist/src/Program.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node-Express\" -c \"blue.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "write:file": "ts-node server/scripts/write-knex-config.ts",
    "scaffold": "npx knex-cli model:generate --name",
    "precommit": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bluebird": "^3.5.37",
    "@types/knex": "^0.16.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "knex": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.2",
    "@types/ramda": "^0.28.15",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "18.1.0"
  }
}
